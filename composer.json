{
    "name": "hopex/simplog",
    "description": "Simple data logging with customize directories.",
    "type": "library",
    "keywords": ["php", "log", "logger", "logging"],
    "version": "1.0.5",
    "require": {
        "php": "^7.4",
        "ext-json": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hopex\\Simplog\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Schizo",
            "email": "hopex.schizo@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Hopex\\Simplog\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "Logger": "Hopex\\Simplog\\Facades\\Logger"
            }
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
